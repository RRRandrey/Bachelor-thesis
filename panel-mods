  log(labor_reg) + humcap_reg + log(Export_reg + 1) + log(Import_reg + 1)


re1 <- plm(formula_no_FDI,
           effect = 'twoways',
           model = 'random',
           index = c('region', 'year'),
           data = df_no_FDI_76)


cov1re <- vcovHC(re1, type = 'HC3')
coeftest(re1, df = Inf, vcov = cov1re)



